@page
@model ScheduleModel
@{
    ViewData["Title"] = "Programar capítulo";
}
<h2>Programar capítulo (uno)</h2>
<form method="post">
    <div><label>ClubId</label> <input asp-for="Input.ClubId" /></div>
    <div><label>BookId</label> <input asp-for="Input.BookId" /></div>
    <div><label>Capítulo</label> <input asp-for="Input.Chapter" type="number" /></div>
    <div><label>Fecha</label> <input asp-for="Input.Date" type="date" /></div>
    <button type="submit">Guardar</button>
</form>
@if (Model.Saved)
{
    <p>Programación creada.</p>
}